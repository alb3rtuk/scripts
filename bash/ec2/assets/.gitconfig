[user]
  name = Albert Rannetsperger
  email = alb3rtuk@me.com

[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  ui = true
  pager = true

[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green

[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold

[color "status"]
  added = yellow
  changed = green
  untracked = cyan

[core]
  pager = less -FRSX
  whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
  editor = nano
	autocrlf = input

[push]
  default = simple

[alias]
  co  = checkout
  com = checkout master
  st  = status
  br  = branch
  cm  = commit